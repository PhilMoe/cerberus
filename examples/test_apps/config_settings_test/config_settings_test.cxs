Function Main:Int()
    New ConfigSettingsTest()
    Return 0
End


Class ConfigSettingsTest Extends App
	
	Field rotation:Float
	
    Method OnCreate:Int()
		Print "Here we test again..."
    	Return 0
    End
    
    
    Method OnUpdate:Int()
    	rotation+=2.0
		Return 0
    End
    
    
    Method OnRender:Int()
    
    	Cls()
    	
    	PushMatrix()
      	Translate(DeviceWidth()*0.5, DeviceHeight()*0.5)
	  	Rotate(-rotation)
    	DrawRect(-100,-100, 200, 200)
    	PopMatrix()
    	Return 0
    End

End